{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "es2015",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": [".dumi/tmp/*"],
      "@hankliu/colors": ["src/index.ts"],
    }
  }
}
