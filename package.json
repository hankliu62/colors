    "eslint-plugin-unicorn": "^52.0.0",
    "father": "^4.4.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
